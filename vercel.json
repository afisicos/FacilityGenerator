{
  "version": 2,
  "buildCommand": "cd mapgenerator && npm run build",
  "outputDirectory": "mapgenerator/dist",
  "installCommand": "cd mapgenerator && npm install",
  "framework": null,
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
